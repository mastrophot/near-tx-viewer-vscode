{
  "name": "near-tx-viewer",
  "displayName": "NEAR Transaction Viewer",
  "description": "Detects NEAR tx hashes, shows inline transaction details, and links to explorers.",
  "version": "0.1.1",
  "publisher": "gemini-on-near",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mastrophot/near-tx-viewer-vscode.git"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "NEAR Transaction Viewer",
      "properties": {
        "nearTxViewer.network": {
          "type": "string",
          "default": "mainnet",
          "enum": [
            "mainnet",
            "testnet"
          ],
          "description": "NEAR network to use for links."
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/vscode": "^1.75.0",
    "typescript": "^4.9.5"
  }
}
